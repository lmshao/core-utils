@PACKAGE_INIT@

include("${CMAKE_CURRENT_LIST_DIR}/coreutilsTargets.cmake")

# Provide legacy variables for compatibility
get_target_property(coreutils_INCLUDE_DIRS coreutils::@PROJECT_NAME@-static INTERFACE_INCLUDE_DIRECTORIES)
if(NOT coreutils_INCLUDE_DIRS)
    get_target_property(coreutils_INCLUDE_DIRS coreutils::@PROJECT_NAME@-shared INTERFACE_INCLUDE_DIRECTORIES)
endif()

# Set found flag
set(coreutils_FOUND TRUE)

# Provide convenient aliases
if(TARGET coreutils::@PROJECT_NAME@-static)
    if(NOT TARGET coreutils::coreutils)
        add_library(coreutils::coreutils ALIAS coreutils::@PROJECT_NAME@-static)
    endif()
    if(NOT TARGET coreutils)
        add_library(coreutils ALIAS coreutils::@PROJECT_NAME@-static)
    endif()
elseif(TARGET coreutils::@PROJECT_NAME@-shared)
    if(NOT TARGET coreutils::coreutils)
        add_library(coreutils::coreutils ALIAS coreutils::@PROJECT_NAME@-shared)
    endif()
    if(NOT TARGET coreutils)
        add_library(coreutils ALIAS coreutils::@PROJECT_NAME@-shared)
    endif()
endif()

check_required_components(coreutils)
